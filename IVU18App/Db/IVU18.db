record(ai, "$(SYS){$(DEV)}P8197") {
  field(DTYP, "stream")
  field(DESC, "Motor1 Pos")
  field(SCAN, "1 second")
  field(EGU, "um")
  field(INP, "@pmac.proto getVar(P8197) $(PORT)")
  field(PREC, "3")
}



